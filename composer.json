{
  "name": "pixeldev/cloudflare-turnstile-bundle",
  "description": "A simple package to help integrate Cloudflare Turnstile on Symfony.",
  "keywords": [
    "symfony bundle",
    "cloudflare-turnstile",
    "form security",
    "captcha"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel Developpement",
      "homepage": "https://www.pixel-developpement.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/form": "^5.0|^6.0",
    "symfony/framework-bundle": "^5.0|^6.0",
    "symfony/http-client": "^5.0|^6.0",
    "symfony/yaml": "^5.0|^6.0",
    "symfony/validator": "^5.0|^6.0",
    "symfony/twig-bundle": "^5.0|^6.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "PixelDev\\CloudflareTurnstileBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-symfony": "^1.2",
    "symplify/easy-coding-standard": "^11.1",
    "rector/rector": "^0.14.5"
  }
}
